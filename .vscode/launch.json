    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug",
            "program": "${workspaceRoot}/src/index.js",
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/babel-node",
            "sourceMaps": true,
            "env": {
                "BABEL_ENV": "debug"
            },
            "outputCapture": "std"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Debug HTTP",
            "program": "${workspaceRoot}/dist/httpserver.js",
            "runtimeExecutable": "node",
            "sourceMaps": true,
            "env": {
                "BABEL_ENV": "debug"
            },
            "outputCapture": "std"
        }
    ]
}